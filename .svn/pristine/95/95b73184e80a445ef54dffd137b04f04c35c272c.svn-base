/* 注册 */
#regsiter{
    background: url("../images/regsiter/bg.jpg") repeat-x;
}
#regsiter .regsiters .zhu1{
    margin-left:900px;
}
#regsiter .regsiters .zhu1 a{
    color:#0066cc;
}
#regsiter .regsiters .regsiters-t{
    display: inline-block;
    text-align: center;
    font: 18px/37px "楷体";
    color:#000;
    border:3px solid #eaeaea;
    border-radius: 5px;
    border-bottom:none;
    width:118px;
    background: #fff;
    position: relative;
    top:3px;
    color:rgb(241, 6, 6);
}
#regsiter .regsiters-c{
    overflow: hidden;
    background: #fff;
    border: 3px solid #eaeaea;
    border-radius: 5px;
    box-shadow: 1px 6px 11px #e4e4e4;
    height: 500px;
    margin: 0 auto;
    padding: 50px 0 0 400px;
}
#regsiter .regsiters-c .wein1 {
    height: 42px;
    line-height: 42px;
    font-size: 16px;
    padding-right: 30px;
    margin-right:20px;
    margin-top:25px;
    overflow: hidden;
}
#regsiter .regsiters-c .wein1 span{
    display: inline-block;
    /* border: none; */
    width:100px;
    height:100px;
    /* border: 1px solid #000; */
    text-align: right;
    float: left;
}

#regsiter .regsiters-c .wein1 .user{
    float: left;
    display: inline-block;
    height: 40px;
    width: 302px;
    border: 1px solid rgb(247, 190, 5);
}
#regsiter .regsiters-c .wein1 .user b{

    display: inline-block;
    width:26px;
    height:26px;
    background: url(../images/semll.png) no-repeat center;
    background-position: -48px 2px;
    position: relative;
    top:5px;
    left:3px;
}
#regsiter .regsiters-c .wein1 p{
    display: inline-block;float: left;
    font: 15px/30px "酷黑软雅";
    color: #e10303;
    margin:10px 0 0 9px;
}
#regsiter .regsiters-c .wein1 .user .b2{background-position: -259px 1px;}
#regsiter .regsiters-c .wein1 .user .b3{background-position: -226px 9pxd;}
#regsiter .regsiters-c .wein1 .user .b4{background-position: -77px 2px;}
#regsiter .regsiters-c .wein1 .user .b5{background-position: -77px 2px;}


input{

    border:none;
    width:250px;
    outline: medium;
    font: 16px/18px "宋体";
}
#buts{
    width: 302px;
    height: 42px;
    font:18px/42px "楷体";
    color: #fff;
    border-radius: 4px;
    border: none;
    background: #e10303;
    outline: medium;
    cursor: pointer;
    margin:30px 0 0 105px;
}
#buts:active{
    background: #f16e6e;
}
#regsiter .regsiters-c .deal{
    line-height: 16px;  
    margin:30px 0 0 105px;
}
#regsiter .regsiters-c .deal a{
    color:#0066cc;
}
#regsiter .regsiters-c .deal p{
    display: inline-block;
    color: #e10303;
    position: relative;
}
#deal{
    width:20px;
    display: inline-block;
    position: relative;
    top:3px;
}
#regsiter .regsiters-c .regsiterok{
    display: none;
    width: 300px;
    height:300px;
    position: relative;
    background: rgb(241, 191, 191);
    left:30px;
    top:-370px;
}
#regsiter .regsiters-c .regsiterok span{
    width:100%;
    display: block;
    font: 30px/220px "酷黑软雅";
    font-weight: 700;
    text-align: center;
    color: rgb(235, 2, 2);
}
#regsiter .regsiters-c .regsiterok .tiaozhuan{
    font: 14px/80px "软雅酷黑";
    text-align: center;
    color: #343434;
}
#regsiter .regsiters-c .regsiterok .tiaozhuan a{
    color: #07b3f7;
}